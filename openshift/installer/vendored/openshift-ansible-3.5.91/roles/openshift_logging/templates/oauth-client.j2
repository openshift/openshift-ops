apiVersion: v1
kind: OAuthClient
metadata:
  name: kibana-proxy
  labels:
    logging-infra: support
secret: {{secret}}
redirectURIs:
- https://{{openshift_logging_kibana_hostname}}
- https://{{openshift_logging_kibana_ops_hostname}}
scopeRestrictions:
- literals:
  - user:info
  - user:check-access
  - user:list-projects
